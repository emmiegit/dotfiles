def_key "k"
    scroll_up

def_key "j"
    scroll_down

def_key "ctrl-u"
    page_up

def_key "ctrl-d"
    page_down

def_key "home"
    move_home

def_key "end"
    move_end

def_key "g"
    move_home

def_key "G"
    move_end

def_key "h"
    jump_to_parent_directory

def_key "l"
    enter_directory

def_key "l"
    run_action

def_key "l"
    play_item

def_key "a"
    add_item_to_playlist

def_key "backspace"
    replay_song

def_key "space"
    pause

def_key "d"
    delete_playlist_items

def_key "<"
    volume_down

def_key ">"
    volume_up

def_key "."
    seek_forward

def_key ","
    seek_backward

def_key "right"
    seek_forward

def_key "left"
    seek_backward

def_key "n"
    next

def_key "N"
    previous

def_key "n"
    next_found_item

def_key "N"
    previous_found_item

def_key ":"
    execute_command

def_key "B"
    jump_to_playlist_editor

def_key "e"
    edit_song

def_key "E"
    jump_to_tag_editor

def_key "P"
    save_tag_changes

def_key "ctrl-b"
    sort_playlist
